(version 1)
(rule "class 0:0"
  (condition "A.NetClass == 'default' && B.NetClass == 'Default'")
  (constraint clearance (min 0.1524mm)))

(rule "class 2:2"
  (condition "A.NetClass == 'gnd' && B.NetClass == 'gnd'")
  (constraint clearance (min 0.254mm)))

(rule "class 3:3"
  (condition "A.NetClass == 'usbvcc' && B.NetClass == 'usbvcc'")
  (constraint clearance (min 0.254mm)))

(rule "class 4:4"
  (condition "A.NetClass == 'USBpair' && B.NetClass == 'USBpair'")
  (constraint clearance (min 0.4064mm)))
